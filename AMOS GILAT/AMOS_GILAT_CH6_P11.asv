% Determine the roots of a quadratic equation

% Inputs for ax^b + bx + c = 0
a = input("Enter a: ");
b = input("Enter b: ");
c = input("Enter c: ");

% Calculate Delta
delta = b^2 - 4*a*c;

if delta
    x1 = -b + sqrt(delta) / 2*a;
    x2 = -b - sqrt(delta) / 2*a;

    if delta<0
        warning("The roots will be imaginary\n");
    end
    
    disp("X1: "); disp(x1);
    disp("X2: "); disp(x2);

else
    x = -b / 2*a;
    fprintf("X: %.2f\n",x);
    
end